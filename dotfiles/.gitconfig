[gpg]
	program = /usr/local/bin/gpg
[commit]
	gpgsign = true
[init]
	defaultBranch = main
[url "git@github.com:"]
	insteadOf = https://github.com/
[pull]
	rebase = false
[core]
	editor = code --wait
[diff]
  tool = vscode
[difftool "vscode"]
  cmd = code --wait --diff $LOCAL $REMOTE
[merge]
  tool = vscode
[mergetool "vscode"]
  cmd = code --wait $MERGED
[includeIf "gitdir:/Users/s.nance/"]
  path = ~/src/llwt/configs/dotfiles/.gitconfig-work